{
  "name": "with-storage",
  "displayName": "With storage",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "coldsauce",
  "scripts": {
    "dev:chrome": "plasmo dev",
    "dev:firefox": "plasmo dev --target=firefox-mv2",
    "build:chrome": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv2"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.6.1",
    "plasmo": "^0.73.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "*",
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  }
}
