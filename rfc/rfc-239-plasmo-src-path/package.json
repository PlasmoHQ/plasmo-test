{
  "name": "rfc-239-plasmo-src-path",
  "displayName": "Rfc 239 plasmo src path",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "lgvic",
  "scripts": {
    "dev": "plasmo dev --src-path=src/extension",
    "build": "plasmo build --src-path=src/extension"
  },
  "dependencies": {
    "plasmo": "0.73.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs"
    ]
  }
}
