{
  "name": "rfc-182",
  "displayName": "Rfc 182",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "lgvic",
  "packageManager": "pnpm@7.11.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "plasmo": "0.73.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
