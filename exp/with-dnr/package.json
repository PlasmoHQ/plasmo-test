{
  "name": "with-content-scripts-ui",
  "displayName": "With content scripts ui",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "lgvic",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "plasmo": "0.57.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.3.1",
    "@types/chrome": "0.0.198",
    "@types/node": "18.11.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "~rulesets/test.json"
        }
      ]
    }
  }
}
